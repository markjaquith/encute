{
  "name": "@markjaquith/encute",
  "description": "Fluent management of scripts and styles.",
  "version": "0.8.0",
  "license": "GPL-2.0-or-later",
  "private": true,
  "keywords": [],
  "scripts": {
    "build": "rollup -c",
    "dev": "rollup -c -w",
    "build:production": "rollup -c --no-sourcemap",
    "start": "sirv dist --no-clear",
    "clean-dist": "git clean -f dist",
    "composer:production": "composer install && composer install --no-dev --no-scripts",
    "make-zip": "wp dist-archive . encute.zip",
    "package": "yarn run clean-dist && yarn run build:production && yarn run composer:production && yarn run make-zip && composer install"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^17.0.0",
    "@rollup/plugin-node-resolve": "^11.0.0",
    "rollup": "^2.3.4",
    "rollup-plugin-css-only": "^3.1.0",
    "rollup-plugin-livereload": "^2.0.0",
    "rollup-plugin-svelte": "^7.0.0",
    "rollup-plugin-terser": "^7.0.0",
    "svelte": "^3.44.0"
  },
  "dependencies": {
    "sirv-cli": "^1.0.14"
  }
}
